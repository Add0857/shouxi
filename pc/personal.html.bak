$('.promotBtn').click(function(){
	if($(this).parents('.promoteCard').find('.textContent').is(':visible')){
	$(this).parents('.promoteCard').find('.textContent').hide();
	}else{
	$(this).parents('.promoteCard').find('.textContent').show();
	}
});


$('.finance li').click(function(){
	var index = $(this).index();
	$('.finance li a').removeClass('active');
	$(this).find('a').addClass('active');
	$('#historyList .histrans').hide();
	$('#historyList .histrans').eq(index).show();
});

});
</script>
