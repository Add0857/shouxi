<script>
function plusZero (string) {
        if (string < 10) {
            string = '0' + string;
        }
        return string;
} 
function getCurrentTime()
{
        var time = new Date();
        var gmt_time = document.getElementById('gmt_time');
        gmt_time.children[1].innerText = plusZero(time.getHours());
        gmt_time.children[2].innerText = plusZero(time.getMinutes());
        gmt_time.children[3].innerText = plusZero(time.getSeconds());
        setTimeout("getCurrentTime()", 1000);
    }
</script>
<script type="text/javascript">
  function autoScroll(obj){
    $(obj).find("ul").animate({
      marginTop : "-17px"
    },1000,function(){
      $(this).css({marginTop : "0px"}).find("li:first").appendTo(this);
    })
  }
  $(function(){
    var scroll=setInterval('autoScroll("#box")',2000);
     $("#box").hover(function(){
       clearInterval(scroll);
     },function(){
      scroll=setInterval('autoScroll("#box")',2000);
     });
  });
</script>
