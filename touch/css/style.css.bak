@charset "utf-8";
/* reset css */
body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, form, input, textarea, p, th, td {margin:0; padding:0; -webkit-text-size-adjust:none;}
th{font-weight:normal; text-align:left;}
table{border-collapse:collapse; border-spacing:0;}
img{border:0;}
ol, ul{list-style:none;}
a{outline:none; hlbr:expression(this.onFocus=this.blur()); text-decoration:none; cursor:pointer;}
input , select , textarea , button{outline:none; border-radius:0; font-family:arial , "微軟正黑體";}
input[type="number"]::-webkit-inner-spin-button ,
input[type="date"]::-webkit-inner-spin-button{-webkit-appearance:none;}
button{padding:0; cursor:pointer;}

body{padding-top:60px; background:#f2f2f2; font-family:arial , "微軟正黑體";}
#all.active{position:fixed; width:100%;}

/* header */
#header{position:fixed; left:0; top:0; width:100%; padding:20px 0; background:#FFF; overflow:hidden; box-shadow:0 0 10px rgba(0,0,0,.75); z-index:8;background-image: url(../images/bg1.png) !important;}
#header .member{position:relative; float:right;}
#header .member a{display:inline-block; margin-right:10px; font-size:15px; color:#fff;}
#header .member a:before{content:""; display:inline-block; width:12px; height:12px; margin-right:5px; background:url(../images/header_icon.png);}
#header .member a.login:before{background-position:left top;}
#header .member a.register:before{background-position:right top;}

#header .msg{position:relative; float:right;}
#header .msg a{display:inline-block; margin-right:10px; font-size:15px; color:#666;}
#header .msg a:before{content:""; position:relative; top:1px; display:inline-block; width:20px; height:16px; margin-right:5px; background:url(../images/msg_icon.png);}
#header .msg a.normal:before{background-position:left top;}
#header .msg a.alert:before{background-position:right top;}

#header .deposit{position:relative; float:right; display:inline-block; margin:0 10px; font-size:15px; color:#fff;}
#header .deposit:before{content:""; display:inline-block; width:15px; height:15px; margin-right:5px; background:url(../images/deposit.png);}
#header .point{position:relative; float:right; font-size:15px; color:#18ea18;}

#header .back{position:relative; float:left; display:block; width:9px; height:16px; margin:4px 0 0 15px; background:url(../images/back.png); text-indent:-9999px;}


#logo{position:absolute; left: 59px; top:15px; width:106px; height:34px; margin-left:-53px; background:url(../images/logo.png); text-indent:-9999px;}
#main-title{position:absolute; left:0; top:18px; width:100%; padding:0 120px; font-size:18px; color:yellow; font-weight:bold; text-align:center; box-sizing:border-box}

/* navigation */
#navigation{position:fixed; left:0; bottom:0; width:100%; background:#01b1e6; overflow:hidden; box-shadow:0 0 5px rgba(0,0,0,.75); z-index:8;}
#navigation li{float:left; width:20%; text-align:center; box-sizing:border-box;}
#navigation li a{display:inline-block; padding:10px 0; font-size:14px; color:rgba(255,255,255,.7);}
#navigation li.navi-1{background:linear-gradient(#01b1e6 , #1d4db7); border-right:1px solid #001e2c;}
#navigation li a:before{content:""; display:block; width:35px; height:35px; margin:0 auto 8px auto; background:url(../images/navi_icon.png);}
#navigation li.navi-1 a:before{background-position:left top;}
#navigation li.navi-2 a:before{background-position:-35px top;}
#navigation li.navi-3 a:before{background-position:-70px top;}
#navigation li.navi-4 a:before{background-position:-105px top;}
#navigation li.navi-5 a:before{background-position:right top;}

/* mobile */
#mobi-btn{position:relative; float:left; display:block; width:20px; height:15px; margin:5px 0 0 15px; text-indent:-9999px;}
#mobi-btn:before ,
#mobi-btn hr ,
#mobi-btn:after{content:""; position:absolute; left:0; display:block; width:100%; height:3px; background:yellow;}
#mobi-btn:before{top:0;}
#mobi-btn hr{top:5px; margin:0; border:0;}
#mobi-btn:after{top:10px;}

#mask{position:fixed; left:0; top:0; display:none; width:100%; height:100%; background:rgba(0,0,0,.5); text-indent:-9999px; z-index:9;}

#mobile{position:absolute; left:-290px; top:0; width:280px; height:100%; padding-bottom:20px; background:#003148; overflow-y:auto; box-shadow:5px 0 10px rgba(0,0,0,.5); box-sizing:border-box; z-index:10;}
#mobile .member{position:relative; min-height:64px; margin-bottom:10px; padding:20px 20px 20px 100px; box-sizing:border-box;}
#mobile .member dt{position:absolute; left:20px; top:20px; width:64px; height:64px; border-radius:50%; overflow:hidden;}
#mobile .member dt img{display:block; width:100%; height:auto;}
#mobile .member dd .id{padding:10px 0 5px 0; font-size:18px; color:#FFF; font-weight:bold;}
#mobile .member dd .type{font-size:14px; color:#fac504;}
#mobile .member dd .type span{color:#00c4ff;}

#mobile .control{margin-bottom:30px; padding:0 20px; overflow:hidden;}
#mobile .control a{float:left; display:block; width:48%; margin:0 2% 2% 0; padding:10px; background:#0b6189; border-radius:5px; font-size:15px; color:#FFF; letter-spacing:.5px; box-sizing:border-box;}
#mobile .control a:before{content:""; display:inline-block; width:25px; height:25px; margin-right:5px; background:url(../images/mobile_control.png); vertical-align:middle;}
#mobile .control a.btn-1:before{background-position:left top;}
#mobile .control a.btn-2:before{background-position:-25px top;}
#mobile .control a.btn-3:before{background-position:-50px top;}
#mobile .control a.btn-4:before{background-position:right top;}

#mobile .game-list{margin-bottom:20px;}
#mobile .game-list > li{border-bottom:1px solid #03233d;}
#mobile .game-list > li > a{display:block; padding:10px 0; font-size:17px; color:#FFF; letter-spacing:.5px; text-indent:20px;}
#mobile .game-list > li > a:before{content:""; display:inline-block; width:27px; height:27px; margin-right:15px; background:url(../images/mobile_game.png); vertical-align:middle;}
#mobile .game-list > li.type-1 > a:before{background-position:left top;}
#mobile .game-list > li.type-2 > a:before{background-position:-27px top;}
#mobile .game-list > li.type-3 > a:before{background-position:-54px top;}
#mobile .game-list > li.type-4 > a:before{background-position:right top;}
#mobile .game-list .sub-menu{display:none;}
#mobile .game-list .sub-menu li{padding:0 20px 0 60px; background:#0c1217;}
#mobile .game-list .sub-menu li a{display:inline-block; padding:10px 0; font-size:15px; color:#378db5; letter-spacing:.5px;}
#mobile .game-list .sub-menu li span{display:inline-block; font-size:13px; color:#e23d43; letter-spacing:.25px;}
#mobile .game-list .sub-menu li span:before{content:""; display:inline-block; width:16px; height:14px; margin-right:5px; background:url(../images/fix_alert.png) right top; vertical-align:middle;}

#mobile .link{text-align:center;}
#mobile .link li{display:inline-block; padding:0 30px;}
#mobile .link li.log{border-right:1px solid #03233d;}
#mobile .link li a{font-size:14px; color:#b6d0dc; letter-spacing:.5px;}
#mobile .link li a:before{content:""; display:block; width:34px; height:34px; margin:0 auto 10px auto; background:url(../images/mobile_link.png);}
#mobile .link li.log a:before{background-position:left top;}
#mobile .link li.media a:before{background-position:right top;}

/* slider */
#slider .owl-nav > div{position:absolute; top:0; width:40px; height:100%; background:rgba(0,0,0,.5);}
#slider .owl-nav > div:before{content:""; position:absolute; left:16px; top:50%; display:block; width:8px; height:14px; margin-top:-7px; background:url(../images/index_slider_arrow.png);}  
#slider .owl-nav .owl-prev{left:0;}
#slider .owl-nav .owl-prev:before{background-position:left top;}
#slider .owl-nav .owl-next{right:0;}
#slider .owl-nav .owl-next:before{background-position:right top;}

/* news */
#news{position:relative; padding:0 15px; background:#063a65; overflow:hidden; line-height:40px;}
#news span{font-size:16px; color:#fdb415; letter-spacing:.5px;}
#news span:before{content:""; position:relative; top:3px; display:inline-block; width:14px; height:16px; margin-right:10px; background:url(../images/index_news.png);}
/* #news span:after{content:""; position:relative; top:-2px; display:inline-block; width:0; height:0; margin-left:10px; border-style:solid; border-width:3.5px 0 3.5px 5px; border-color:transparent transparent transparent #fdb415} */
#news ul{position:absolute; left:40px; right:70px;}
#news li b{font-size:16px; color:#fdb415; letter-spacing:.5px;}
#news li b:after{content:""; position:relative; top:-2px; display:inline-block; width:0; height:0; margin-left:10px; border-style:solid; border-width:3.5px 0 3.5px 5px; border-color:transparent transparent transparent #fdb415}
#news li a{display:block; font-size:15px; color:#bac2ca; white-space:nowrap; overflow:hidden; text-overflow:ellipsis; transition:all .3s;}
#news li a:hover{color:#FFF;}
#news .nav{position:absolute; right:15px; top:12px;}
#news .nav a{float:left; display:block; width:18px; height:17px; margin-left:15px; background:url(../images/index_news_arrow.png); text-indent:-9999px;}
#news .nav a.prev{background-position:left top;}
#news .nav a.next{background-position:right top;}

/* index-game */
#index-game h2{padding:8px 0; background:linear-gradient(#225ad8 , #16c1c1); border-top:1px solid #FFF; border-bottom:1px solid #BBB; font-size:16px; color:#333; text-align:center;}
/*#index-game h2:before{content:""; display:inline-block; width:22px; height:21px; margin-right:5px; background:url(../images/index_title.png); vertical-align:middle;}*/
#index-game.type-1 h2:before{background-position:left top;}
#index-game.type-2 h2:before{background-position:center top;}
#index-game.type-3 h2:before{background-position:right top;}

#index-game .wrap{background-image: url(../images/bg1.png) !important;padding:15px 0; background:#f7f6f6; border-top:1px solid #FFF; border-bottom:1px solid #BBB; overflow:hidden;}
#index-game .wrap dl{position:relative; float:left; width:25%; padding:0 10px; box-sizing:border-box;}
#index-game .wrap dl a{display:block;}
#index-game .wrap dt{position:relative;}
#index-game .wrap dt img{display:block; width:100%; height:auto; margin-bottom:5px; border-radius:15px; box-shadow:1px 0 2px rgba(0,0,0,.5);}
#index-game .wrap dt .fix{position:absolute; left:0; top:50%; display:block; width:100%; margin-top:-20px; padding:5px 0; background:#e23d42; font-size:12px; color:#FFF; text-align:center;}
#index-game .wrap dt .fix span{display:block; font-size:13px;}
#index-game .wrap dt .fix span:before{content:""; width:16px; height:14px; background:url(../images/fix_alert.png);}
#index-game .wrap dd{font-size:15px; color:#555; font-weight:bold; text-align:center;}

/* get-game */
#get-game{width:100%; height:100%; background:rgba(0,0,0,.9);}
#get-game .wrap{display:table-cell; vertical-align:middle;}
#get-game .enter{margin-top:20px; margin-bottom:20px; text-align:center;}
#get-game .enter a{display:inline-block; margin:0 30px; font-size:16px; color:#FFF;}
#get-game .enter a:before{content:""; display:block; width:77px; height:68px; margin:0 auto 10px auto; background:url(../images/get_game.png);}
#get-game .enter a.web:before{background-position:left top;}
#get-game .enter a.app:before{background-position:right top;}
#get-game .close{display:block; width:230px; margin:0 auto; padding:10px 0; background:#0b6189; font-size:15px; color:#FFF; letter-spacing:.5px; font-weight:bold; text-align:center;}
#get-game .close:before{content:"X"; margin-right:10px;}

/* footer */
#footer{padding:15px 0 100px 0; text-align:center;}
#footer .links li{position:relative; display:inline-block; padding:0 15px;}
#footer .links li.media:after{content:""; position:absolute; right:0; top:3px; display:block; width:1px; height:15px; background:#CCC;}
#footer .links li a{font-size:14px; color:#666; letter-spacing:.5px;}
#footer .links li a:before{content:""; display:inline-block; width:17px; height:16px; margin-right:5px; background:url(../images/footer_link.png); vertical-align:middle;}
#footer .links li.media a:before{background-position:left top;}
#footer .links li.pc a:before{background-position:right top;}
#footer p{padding-top:5px; font-size:12px; color:#999;}

/* sub-title */
.sub-title{padding:8px 20px; font-size:14px; color:#0b6189;}

/* form-style */
input[type="text"].common ,
input[type="number"].common ,
input[type="password"].common{display:block; width:100%; height:33px; padding:0 8px; background:none; border:none; border-bottom:1px solid #ccd1d7; font-size:15px; color:#333; box-sizing:border-box;}
select.common{display:block; width:100%; height:33px; padding:0 8px; border:1px solid #cecece; font-size:15px; color:#333; box-sizing:border-box;}

/* form-tip */
#form-tip{margin-top:10px; font-size:13px;}
#form-tip:before{content:""; display:inline-block; width:10px; height:8px; margin-right:5px; background:url(../images/form_tip.png);}
#form-tip.ok{color:#6bc04e;}
#form-tip.ok:before{background-position:left top;}
#form-tip.wrong{color:#ce3a33;}
#form-tip.wrong:before{background-position:right top;}

/* color */
.green{color:#56c10f;}
.red{color:#ce3a33;}
.blue{color:#0b6189;}
.light-blue{color:#01b1e6;}
.pink{color:#f47ad6;}
.grey{color:#aaa;}
.orange{color:#f28900;}

/* blue-btn */
#blue-btn{padding:10px 20px; background:#f2f2f2; transition:all .3s;}
#blue-btn a{display:block; background:#0b6189; border-radius:3px; font-size:16px; color:#FFF; line-height:40px; letter-spacing:.5px; text-align:center; transition:all .3s;}
#blue-btn.float{position:fixed; left:0; bottom:82px; width:100%; padding:0;}
#blue-btn.float a{border-radius:0;}
/* filter */
#filter{margin-bottom:5px; border-top:1px solid #ccd1d7; border-bottom:1px solid #ccd1d7; overflow:hidden; box-shadow:0 1px 1px rgba(0,0,0,.1);}
#filter select{float:left; width:33.33%; height:40px; padding-left:15px; background:#fafafa; border:none; border-right:1px solid #ccd1d7; font-size:15px; color:#333; text-align:center; box-sizing:border-box;}
#filter button{float:left; width:33.34%; height:40px; background:#01b1e6; border:none; font-size:15px; color:#FFF; text-align:center;}
#filter button:after{content:""; display:inline-block; width:13px; height:14px; margin-left:10px; background:url(../images/search.png); vertical-align:middle;}

/* page */
#page{width:154px; margin:0 auto; padding:20px 0; overflow:hidden;}
#page a{float:left; display:block; width:32px; height:32px; background:url(../images/arrow_page.png) #FFF; border:1px solid #bbb; border-radius:5px; text-indent:-9999px; box-sizing:border-box;}
#page a.prev{background-position:left top;}
#page a.next{background-position:right top;}
#page select{float:left; width:80px; height:32px; margin:0 5px; padding:0 10px; border:1px solid #bbb; border-radius:5px; box-sizing:border-box;}

/* empty-display */
#empty-display{padding:60px 0; background:#f2f2f2; font-size:16px; color:#999; text-align:center;}
#empty-display:before{content:""; display:block; width:33px; height:40px; margin:0 auto 5px auto; background:url(../images/empty.png);}

/* notice */
#notice{padding:20px; font-size:14px; color:#666; line-height:21px;}
#notice li{margin-bottom:5px; padding-left:12px; text-indent:-12px;}
#notice li span{color:#0b6189;}

/* green-notice */
#green-notice{margin-top:10px; padding:10px; background:#7bbd65; border-radius:5px; font-size:13px; color:#FFF; line-height:18px;}

/* contact */
#contact{border-bottom:1px solid #BBB;}
#contact .wrap{padding:15px 10px; background:#f9f9f9; border-top:1px solid #FFF; overflow:hidden;}
#contact .wrap a{display:inline-block; width:61px; height:61px; margin:0 10px; background:url(../images/contact_icon.png); text-indent:-9999px;}
#contact .wrap a.talk{background-position:left top;}
#contact .wrap a.line{background-position:-61px top;}
#contact .wrap a.wechat{background-position:-122px top;}
#contact .wrap a.qq{background-position:right top;}

/* member-center */
#memberHome dl{position:relative; display:table; width:100%; margin-bottom:5px; padding:15px; background:#003148; box-sizing:border-box;}
#memberHome dt{display:table-cell; width:64px; height:64px; border-radius:50%; overflow:hidden;}
#memberHome dt img{display:block; width:100%; height:auto;}
#memberHome dd{display:table-cell; padding-left:15px; vertical-align:middle; font-size:18px; color:#FFF; font-weight:bold;}
#memberHome dd span{display:block; font-size:14px; color:#fac504; font-weight:normal;}
#member-center .link{background:#fafafa;}
#member-center .link a{display:block; padding:15px 20px; border-bottom:1px solid #ccd1d7; overflow:hidden; font-size:16px; color:#333;}
#member-center .link a:before{content:""; display:inline-block; width:21px; height:20px; margin-right:10px; background:url(../images/member_center.png); vertical-align:middle}
#member-center .link a.btn-1:before{background-position:left top;}
#member-center .link a.btn-2:before{background-position:-21px top;}
#member-center .link a.btn-3:before{background-position:-42px top;}
#member-center .link a.btn-4:before{background-position:-63px top;}
#member-center .link a.btn-5:before{background-position:-84px top;}
#member-center .link a.btn-6:before{background-position:right top;}
#member-center .link a:after{content:""; float:right; display:block; width:6px; height:11px; margin-top:7px; background:url(../images/arrow_grey.png);}

#member-center .app{padding:10px 20px; background:#fafafa; border-bottom:1px solid #ccd1d7;}
#member-center .app li{position:relative; padding:10px 0 10px 90px;}
#member-center .app li.line{font-size:15px;}
#member-center .app li span{position:absolute; left:0; top:19px; font-size:16px; color:#333;}
#member-center .app li.line input{color:#0b6189;}
#member-center .app li.wechat input{color:#999;}
#member-center .app li .copy{float:right; display:block; width:13px; height:15px; background:url(../images/copy.png); text-indent:-9999px;margin-top: 15px;}

#member-center .moneypw{padding:10px 20px; background:#fafafa; border-bottom:1px solid #ccd1d7;}
#member-center .moneypw li{position:relative; padding:10px 0 10px 90px;}
#member-center .moneypw li span{position:absolute; left:0; top:19px; font-size:16px; color:#333;}

/* wallet */
#wallet .record{background:#FFF; border-bottom:1px solid #ccd1d7; overflow:hidden;}
#wallet .record .left ,
#wallet .record .right{display:block; width:50%; font-size:16px; color:#0b6189; line-height:50px; text-align:center; box-sizing:border-box;}
#wallet .record .left{float:left; border-right:1px solid #f2f2f2;}
#wallet .record .right{float:right;}
#wallet .record .left:before ,
#wallet .record .right:before{content:""; display:inline-block; width:13px; height:15px; margin-right:5px; background:url(../images/wallet_record.png); vertical-align:middle;}

#wallet .point{padding:20px 0; background:#fafafa; overflow:hidden;}
#wallet .point .left ,
#wallet .point .right{width:50%; font-size:14px; color:#999; letter-spacing:.5px; text-align:center; box-sizing:border-box;}
#wallet .point .left{float:left; border-right:1px solid #ccd1d7;}
#wallet .point .right{float:right;}
#wallet .point span{display:block; margin:5px 0; font-size:16px; color:#284a2b;}
#wallet .point a{display:inline-block; padding:7px 20px; background:#ce3a33; border-radius:3px; font-size:16px; color:#FFF;}
/*#wallet .point a:before{content:"";display:inline-block; width:20px; height:20px; margin-right:5px; background:url(../images/wallet_control.png); vertical-align:sub}*/
#wallet .point .left a:before{background-position:left top;}
#wallet .point .right a:before{background-position:right top;}

#wallet .list{margin-bottom:5px;}
#wallet .list h2{overflow:hidden;}
#wallet .list h2 .refresh{float:right; display:inline-block; width:16px; height:16px; margin-top:2px; background:url(../images/refresh.png); text-indent:-9999px;}
#wallet .list .wrap{background:#fafafa;}
#wallet .list .wrap li{position:relative; padding:15px 210px 15px 20px; border-bottom:1px solid #ccd1d7; font-size:16px; color:#333;}
#wallet .list .wrap li span{position:absolute; right:110px; top:16px; color:#6983b3;}
#wallet .list .wrap li a{position:absolute; right:20px; top:11px; display:inline-block; padding:5px; border-style:solid; border-width:1px; border-radius:3px; font-size:14px;}
#wallet .list .wrap li a.green{border-color:#7bbd65; color:#7bbd65;}
#wallet .list .wrap li a.yellow{border-color:#f2a90c; color:#f2a90c;}
#wallet .list .wrap li a:after{content:""; position:relative; top:1px; display:inline-block; width:6px; height:11px; margin-left:5px;}
#wallet .list .wrap li a.green:after{background:url(../images/arrow_green.png);}
#wallet .list .wrap li a.yellow:after{background:url(../images/arrow_yellow.png);}

#wallet .control{background:#fafafa;}
#wallet .control .wrap{border-bottom:1px solid #ccd1d7;}
#wallet .control .wrap li{position:relative; padding:10px 20px 10px 100px;}
#wallet .control .wrap li span{position:absolute; left:20px; top:16px; font-size:16px; color:#333;}

/* wallet-deposit */
#wallet-deposit .deposit-way .way{background:#fafafa;}
#wallet-deposit .deposit-way li{position:relative; min-height:54px; padding:10px 55px 10px 120px; border-bottom:1px solid #ccd1d7; box-sizing:border-box;}
#wallet-deposit .deposit-way li label{position:absolute; left:20px; top:16px; display:inline-block; font-size:16px; color:#333;}
#wallet-deposit .deposit-way li label input{display:inline-block; margin-right:5px;}
#wallet-deposit .deposit-way li .tip{position:absolute; right:20px; top:17px; display:inline-block; width:20px; height:20px; background:#01b1e6; border-radius:50%; font-size:12px; color:#FFF; line-height:20px; font-weight:bold; text-indent:-9999px;}
#wallet-deposit .deposit-way li .tip:before{content:"?"; position:absolute; left:6px; top:0; text-indent:0;}
#wallet-deposit .deposit-way li select{max-width:200px;}

#wallet-deposit .deposit-money .money{background:#fafafa;}
#wallet-deposit .deposit-money li{position:relative; height:54px; padding:10px 20px 10px 100px; border-bottom:1px solid #ccd1d7; box-sizing:border-box;}
#wallet-deposit .deposit-money li span{position:absolute; left:20px; top:16px; font-size:16px; color:#333;}

/* payment */
#payment ul{background:#fafafa;}
#payment li{padding:10px 20px; border-bottom:1px solid #ccd1d7; overflow:hidden; font-size:16px; color:#333;}
#payment li .copy{float:right; display:block; width:13px; height:15px; background:url(../images/copy.png); text-indent:-9999px;}


/* wallet-withdrawals */
#wallet-withdrawals .tips dt{padding:10px 20px; background:#0b6189; overflow:hidden; font-size:14px; color:#FFF;}
#wallet-withdrawals .tips dt .refresh{float:right; display:block; width:16px; height:16px; margin-top:2px; background:url(../images/refresh_dark.png); text-indent:-9999px;}
#wallet-withdrawals .tips dd{padding:10px 20px; background:#09afe1;}
#wallet-withdrawals .tips dd a{font-size:16px; color:#FFF;}
#wallet-withdrawals .tips dd a span{color:#ffd428; font-weight:bold;}
#wallet-withdrawals .tips dd a:after{content:""; float:right; display:block; width:6px; height:11px; margin:5px 5px 0 0; background:url(../images/arrow_white.png);}
#wallet-withdrawals .account p{padding:10px 20px; background:#fafafa; border-bottom:1px solid #ccd1d7;}
#wallet-withdrawals .account p span{display:inline-block; margin-left:10px; font-size:13px; color:#999;}
#wallet-withdrawals .point .wrap{background:#fafafa;}
#wallet-withdrawals .point li{position:relative; min-height:54px; padding:10px 20px 10px 120px; box-sizing:border-box;}
#wallet-withdrawals .point li:nth-child(2){padding-right:110px;}

#wallet-withdrawals .point li span{position:absolute; left:20px; top:16px; font-size:16px; color:#333;}
#wallet-withdrawals .point li b{position:relative; top:8px; font-size:15px; color:#ce3a33; font-weight:normal;}
#wallet-withdrawals .point li a{position:absolute; right:20px; top:11px; display:inline-block; padding:5px; border-style:solid; border-width:1px; border-radius:3px; font-size:14px;}
#wallet-withdrawals .point li:nth-child(2) a{border-color:#ce3a33; color:#ce3a33;}
#wallet-withdrawals .point li:nth-child(3) a{border-color:#0b6189; color:#0b6189;}
#wallet-withdrawals .point li a:after{content:""; position:relative; top:1px; display:inline-block; width:6px; height:11px; margin-left:5px;}
#wallet-withdrawals .point li:nth-child(2) a:after{background:url(../images/arrow_red.png);}
#wallet-withdrawals .point li:nth-child(3) a:after{background:url(../images/arrow_blue.png);}

/* wallet-verify */
#wallet-verify dt{padding:10px 20px; background:#0b6189; font-size:14px; color:#FFF;}
#wallet-verify dd{padding:10px 20px; background:#fafafa; border-bottom:1px solid #ccd1d7; font-size:16px; color:#333;}
#wallet-verify .file{padding:10px 20px; background:#fafafa;}
#wallet-verify .wrap{padding:10px 20px; background:#fafafa;} 
#wallet-verify .wrap li{position:relative; min-height:54px; padding:10px 0 10px 80px; box-sizing:border-box;}
#wallet-verify .wrap li span{position:absolute; left:0; top:16px; font-size:16px; color:#333;}

/* betting */
#betting .tabs{background:#FFF; overflow:hidden;}
#betting .tabs a{float:left; display:block; width:100%; border-bottom:2px solid transparent; font-size:16px; color:#333; line-height:50px; text-align:center;}
#betting .tabs a.active{border-bottom-color:#0b6189; color:#0b6189;}

#betting .history dl{margin-bottom:5px; box-shadow:0 1px 1px rgba(0,0,0,.1);}
#betting .history dt{padding:10px 20px; background:#fafafa; font-size:14px; color:#888;}
#betting .history table{width:100%; border-top:1px solid #EEE; border-bottom:1px solid #ccd1d7;}
#betting .history thead{background:#FFF; font-size:14px; color:#888;}
#betting .history tbody{background:#fafafa; font-size:15px; color:#333;}
#betting .history tr{border-bottom:1px solid #EEE;}
#betting .history td{width:33.33%; padding:10px 0; text-align:center; box-sizing:border-box;}
#betting .history tr td:nth-child(2){width:33.34%; border-left:1px solid #EEE; border-right:1px solid #EEE;}

#betting .record dl{margin-bottom:5px; box-shadow:0 1px 1px rgba(0,0,0,.1);}
#betting .record dt{padding:10px 20px; background:#fafafa; font-size:14px; color:#888;}
#betting .record table{width:100%; border-top:1px solid #EEE; border-bottom:1px solid #ccd1d7;}
#betting .record thead{background:#FFF; font-size:14px; color:#888;}
#betting .record tr{border-bottom:1px solid #EEE;}
#betting .record td{width:50%; padding:10px 20px; background:#FFF; border-right:1px solid #EEE; font-size:15px; color:#333; box-sizing:border-box;}
#betting .record td span{display:inline-block; width:60px; margin-right:10px; color:#333;}
#betting .record tr td:nth-child(2){border-right:none;}

#betting .total ul{padding:0 20px; background:#fafafa;}
#betting .total li{border-bottom:1px dashed #ccd1d7; overflow:hidden;}
#betting .total li:last-child{border-bottom:none;}
#betting .total li span{float:right; margin-top:2px;}

#betting .total .wrap li{padding:15px 0; font-size:16px; color:#333;}
#betting .total .list li{padding:10px 0; font-size:15px; color:#666;}
#betting .total .list li:first-child{border-top:1px solid #BBB;}

/* toggle-panel */
#toggle-panel{}

#toggle-panel .total{margin-bottom:5px; box-shadow:0 1px 1px rgba(0,0,0,.1);}
#toggle-panel .total ul{padding:0 20px; background:#fafafa;}
#toggle-panel .total li{border-bottom:1px dashed #ccd1d7; overflow:hidden;}
#toggle-panel .total li:last-child{border-bottom:none;}
#toggle-panel .total li span{float:right; margin-top:2px;}

#toggle-panel .total .wrap li{padding:15px 0; font-size:16px; color:#333;}
#toggle-panel .total .list li{padding:10px 0; font-size:15px; color:#666;}
#toggle-panel .total .list li:first-child{border-top:1px solid #BBB;}

#toggle-panel .toggle{margin-bottom:5px; box-shadow:0 1px 1px rgba(0,0,0,.1);}
#toggle-panel .toggle dt{position:relative; padding:10px 40px 10px 110px; background:#fafafa; border-bottom:1px solid #ccd1d7;}
#toggle-panel .toggle dt:after{content:""; position:absolute; right:20px; top:25px; display:block; width:11px; height:6px; background:url(../images/arrow_toggle.png) left top;}
#toggle-panel .toggle dl.active dt:after{background-position:left bottom;}
#toggle-panel .toggle dt .time{position:absolute; left:20px; top:12px; width:75px; font-size:14px; color:#888;}
#toggle-panel .toggle dt .info .item{font-size:14px; color:#333;}
#toggle-panel .toggle dt .info .value{font-size:12px; color:#888; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
#toggle-panel .toggle dd{display:none; padding:10px 20px; background:#FFF;}
#toggle-panel .toggle dd .wrap h2{margin-bottom:5px; padding-bottom:5px; border-bottom:1px solid #bbb; font-size:14px; color:#aaa; font-weight:normal;}
#toggle-panel .toggle dd .wrap p{border-bottom:1px dashed #ccd1d7; font-size:15px; color:#333; line-height:36px;}
#toggle-panel .toggle dd .wrap p:last-child{border:none;}

#toggle-panel .toggle dd .result{border-top:3px solid #ddd;}
#toggle-panel .toggle dd .result li{padding:10px 0; border-bottom:1px dashed #ccd1d7; overflow:hidden; font-size:15px; color:#666;}
#toggle-panel .toggle dd .result li span{float:right; margin-top:2px;}
#toggle-panel .toggle dd .result li:last-child{border-bottom:none;}

#toggle-panel .toggle dd .list{}
#toggle-panel .toggle dd .list li{position:relative; padding:10px 0 10px 105px; border-bottom:1px dashed #ccd1d7; font-size:15px; color:#333;}
#toggle-panel .toggle dd .list li span{position:absolute; left:0; top:9px; color:#999;}
#toggle-panel .toggle dd .list li:last-child{border-bottom:none;}
#toggle-panel .toggle dd .list li.code{overflow:hidden; color:#0b6189;}
#toggle-panel .toggle dd .list li.code .copy{float:right; display:block; width:13px; height:15px; background:url(../images/copy.png); text-indent:-9999px;}

/* register */
#register ul{padding:10px 20px; background:#fafafa; border-bottom:1px solid #ccd1d7; font-size:16px; color:#333;}
#register li{position:relative; padding:10px 0 10px 90px;}
#register li span{position:absolute; left:0; top:19px;}

#register li.check{padding-left:0;}
#register li.check input{margin-right:5px;}
#register li.check a{color:#01b1e6;}

#register .btn{padding:10px 20px; overflow:hidden;}
#register .btn button{width:47%; display:block; background:#0b6189; border:none; border-radius:3px; font-size:16px; color:#FFF; line-height:40px; letter-spacing:.5px; text-align:center;}
#register .btn button[type="reset"]{float:left;}
#register .btn button[type="submit"]{float:right;}

#register .link{padding:10px; color:#CCC; text-align:center;}
#register .link a{font-size:16px; color:#333;}

/* phone-verify */
#phone-verify{padding:20px; background:#fafafa; border-bottom:1px solid #ccd1d7; text-align:center;}
#phone-verify .code{font-size:16px; color:#333;}
#phone-verify .code input{max-width:200px; margin:0 auto 15px auto; text-align:center;}
#phone-verify .code a{display:inline-block; margin-bottom:10px; padding:5px 25px; background:#FFF; border:1px solid #0b6189; border-radius:5px; font-size:16px; color:#0b6189;}
#phone-verify .code a:after{content:""; display:inline-block; width:6px; height:11px; margin-left:5px; background:url(../images/arrow_blue.png);}
#phone-verify p{font-size:14px; color:#666; line-height:24px;}

/* game-confirm */
#game-confirm .point{padding:10px 20px; background:#09afe1; font-size:16px; color:#FFF;}
#game-confirm .point span{color:#ffd428; font-weight:bold;}
#game-confirm .control{padding:10px 20px; background:#fafafa;}
#game-confirm .control li{position:relative; padding:15px 0 15px 80px; font-size:16px; color:#ce3a33;}
#game-confirm .control li span{position:absolute; left:0; top:20px; color:#333}

#game-confirm .control{border-bottom:1px solid #ccd1d7;}
#game-confirm .control li.money{padding-right:140px;}
#game-confirm .control li.money span{top:12px;}
#game-confirm .control li.money a{position:absolute; right:20px; top:8px; display:inline-block; padding:5px; border-style:solid; border-width:1px; border-radius:3px; font-size:14px;}
#game-confirm .control li.money a:after{content:""; display:inline-block; width:6px; height:11px; margin-left:5px;}
#game-confirm .control li.money a.green{right:0; border-color:#56c10f;}
#game-confirm .control li.money a.green:after{background:url(../images/arrow_green.png);}
#game-confirm .control li.money a.red{right:85px; border-color:#ce3a33;}
#game-confirm .control li.money a.red:after{background:url(../images/arrow_red.png);}

#game-confirm .wrap{padding:20px;}
#game-confirm .wrap .note{padding:15px; background:#f1c824; border-radius:3px; color:#FFF; text-align:center;}
#game-confirm .wrap .note h2{font-size:16px;}
#game-confirm .wrap .note p{font-size:14px;}

#game-confirm .wrap .btn{padding:15px 0; overflow:hidden;}
#game-confirm .wrap .btn a{width:48%; display:block; background:#0b6189; border-radius:3px; font-size:16px; color:#FFF; line-height:40px; letter-spacing:.5px; text-align:center;}
#game-confirm .wrap .btn a:first-child{float:left;}
#game-confirm .wrap .btn a:last-child{float:right;}


/* game-list */
#game-list .link{width:100%; border-bottom:1px solid #ccd1d7; overflow-x:scroll; white-space:nowrap;}
#game-list .link a{display:inline-block; height:50px; padding:0 15px; border-bottom:2px solid transparent; font-size:16px; color:#333; line-height:50px; box-sizing:border-box;}
#game-list .link a.active{border-bottom-color:#0b6189; color:#0b6189;}
#ascrail2000-hr{display:none !important;}
#game-list .search{position:relative; padding:10px 40px 10px 20px;}
#game-list .search button{position:absolute; right:20px; top:10px; display:block; width:13px; height:14px; background:url(../images/search2.png); border:none; text-indent:-9999px;}
#game-list .wrap dl{display:table; width:100%; padding:15px 20px; background:#fafafa; border-bottom:1px solid #ccd1d7; box-sizing:border-box;}
#game-list .wrap dt{display:table-cell; width:100px; vertical-align:middle;}
#game-list .wrap dt img{display:block; width:100%; height:auto; border:1px solid #ccd1d7;}
#game-list .wrap dd{position:relative; display:table-cell; padding:0 15px; vertical-align:middle; overflow:hidden}
#game-list .wrap dd h2{font-size:18px; color:#333; font-weight:normal; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
#game-list .wrap dd .control a{font-size:14px;}
#game-list .wrap dd .control a:before{content:""; display:inline-block; width:16px; height:16px; margin-right:5px; vertical-align:middle;}
#game-list .wrap dd .control a.favorite{margin-right:10px; color:#999;}
#game-list .wrap dd .control a.favorite:before{background-image:url(../images/favorite.png); background-position:left top;}
#game-list .wrap dd .control a.favorite.active{color:#0b6189;}
#game-list .wrap dd .control a.favorite.active:before{background-position:right top;}
#game-list .wrap dd .control a.play{color:#01b1e6;}
#game-list .wrap dd .control a.play:before{background:url(../images/play.png);}
#game-list .wrap dd .enter{position:absolute; right:0; top:50%; display:block; width:6px; height:11px; margin-top:-5px; background:url(../images/arrow_grey.png); text-indent:-9999px;}

/* event */
#event dl{border-bottom:1px solid #ccd1d7;}
#event dt img{display:block; width:100%; height:auto;}
#event dd{position:relative; padding:15px 110px 15px 20px; background:#fafafa; font-size:16px; color:#333; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
#event dd span{position:absolute; right:20px; top:9px; display:inline-block; padding:5px; border-style:solid; border-width:1px; border-radius:3px; font-size:14px;}
#event dd span:after{content:""; display:inline-block; width:6px; height:11px; margin-left:5px;}
#event dd span.green{border-color:#56c10f; color:#56c10f;}
#event dd span.green:after{background:url(../images/arrow_green.png);}
#event dd span.blue{border-color:#0b6189; color:#0b6189;}
#event dd span.blue:after{background:url(../images/arrow_blue.png);}

/* event-detail */
#event-detail .title-1{padding:10px 20px; background:#0b6189; font-size:17px; color:#FFF;}
#event-detail .title-2{margin-bottom:20px; padding:5px 0; border-top:1px dashed #ccd1d7; border-bottom:1px dashed #ccd1d7; font-size:15px; color:#333; font-weight:normal; text-align:center;}
#event-detail .wrap{padding:20px; background:#fafafa;}
#event-detail .wrap .intro{margin-bottom:20px;}
#event-detail .wrap .intro img{display:block; width:100%; height:auto; margin-bottom:10px;}
#event-detail .wrap .intro p{position:relative; padding-left:75px; font-size:15px; color:#0b6189; line-height:30px;}
#event-detail .wrap .intro p span{position:absolute; left:0; top:0; color:#666;}
#event-detail .wrap table{width:100%; margin:10px 0; border-left:1px solid #ccd1d7; border-top:1px solid #ccd1d7; box-sizing:border-box;}
#event-detail .wrap thead{font-size:14px;}
#event-detail .wrap thead .col-1{background:#d5d8dc;}
#event-detail .wrap thead .col-2 ,
#event-detail .wrap thead .col-3{background:#0b6189; color:#FFF;}
#event-detail .wrap tbody{font-size:13px;}
#event-detail .wrap tbody .col-1{background:#f1f1f1; text-align:right;}
#event-detail .wrap td{padding:10px; border-right:1px solid #ccd1d7; border-bottom:1px solid #ccd1d7; text-align:center; box-sizing:border-box;}
#event-detail .wrap .img{display:block; margin-bottom:20px;}
#event-detail .wrap .img img{display:block; width:100%; height:auto;}
#event-detail .wrap .notice{list-style:decimal; margin-bottom:20px; padding-left:20px; font-size:14px; color:#666; line-height:21px;}
#event-detail .wrap .notice li{margin-bottom:5px;}